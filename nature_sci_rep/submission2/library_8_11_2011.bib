title = {{Cooperative nonlinearities in auditory cortical neurons.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2535914\&tool=pmcentrez\&rendertype=abstract},
volume = {58},
year = {2008}
