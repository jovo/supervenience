dispersion relation.},
keywords = { bogoliubov transformation, quantum mechanics, superfluidity,physics},
title = {{Superfluidity}},
year = {2006}
}
@article{Hauser1994,
abstract = {Using the cluster expansions for n-point Green functions we derive
