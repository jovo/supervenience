@article{Wlcek2008,
abstract = {Organic anion transporter polypeptides (OATPs) mediate the transmembrane
uptake of endogenous compounds and clinically important drugs in
various tissues thereby effecting drug disposition and tissue penetration.
OATPs have also been identified in gastric, pancreatic and colon
carcinomas but little is known about their expression in breast carcinoma.
