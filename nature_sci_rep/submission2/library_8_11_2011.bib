journal = {Journal of Neuroscience Methods},
keywords = { clustering, electron microscopy, gaussian classes, image processing, neurobiology, segmentation,computational},
title = {{Contour-Propagation Algorithms for Semi-Automated Reconstruction of Neural Processes}},
year = {2007}
}
