journal = {NeuroImage},
keywords = {Adult,Aged,Aged, 80 and over,Aging,Aging: physiology,Algorithms,Alzheimer Disease,Alzheimer Disease: pathology,Atrophy,Brain Mapping,Brain Mapping: methods,Cerebral Cortex,Cerebral Cortex: pathology,Corpus Callosum,Corpus Callosum: pathology,Dominance, Cerebral,Dominance, Cerebral: physiology,Female,Humans,Image Processing, Computer-Assisted,Image Processing, Computer-Assisted: methods,Imaging, Three-Dimensional,Imaging, Three-Dimensional: methods,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Male,Middle Aged,Observer Variation,Reproducibility of Results,Software,Statistics as Topic},
month = jul,
