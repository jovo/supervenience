journal = {Nature neuroscience},
keywords = {Action Potentials,Action Potentials: physiology,Animals,Brain,Brain: cytology,Brain: physiology,Electrophysiology,Electrophysiology: methods,Humans,Microelectrodes,Nerve Net,Nerve Net: physiology,Neurons,Neurons: physiology,Neurophysiology},
month = may,
