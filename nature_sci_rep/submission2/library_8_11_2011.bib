keywords = {Animals,Calcium,Calcium: metabolism,Fluorescent Dyes,Microscopy, Confocal,Models, Statistical,PC12 Cells,PC12 Cells: metabolism,Rats,Signal Processing, Computer-Assisted,Time Factors},
