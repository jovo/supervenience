pages = {401--17},
pmid = {8423257},
title = {{A model for the responses of low-frequency auditory-nerve fibers in cat.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/8423257},
volume = {93},
year = {1993}
}
