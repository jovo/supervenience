issn = {1530-7026},
journal = {Cognitive, affective \& behavioral neuroscience},
keywords = {Adult,Cerebral Cortex,Cerebral Cortex: anatomy \& histology,Female,Form Perception,Humans,Image Processing, Computer-Assisted,Magnetic Resonance Imaging,Male,Questionnaires,Touch,Visual Cortex,Visual Cortex: physiology,Visual Perception},
