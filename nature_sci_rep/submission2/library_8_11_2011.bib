@article{Svoboda2008h,
abstract = {BACKGROUND: Although the use of potable water for wound irrigation
is attractive in an austere environment, its effectiveness has not
been tested. We sought to compare the effectiveness of potable water
irrigation in reducing bacterial number with that of normal saline
irrigation. METHODS: We used an established caprine model involving
the creation of a reproducible complex musculoskeletal wound followed
by inoculation with luminescent bacteria that allowed for quantitative
analysis with a photon-counting camera system. Six hours after injury
and inoculation, wound irrigations were performed using pulsatile
lavage. Fourteen goats were randomized into two treatment groups:
irrigation with 9 L potable water versus irrigation with 9 L normal
saline. Images obtained after irrigation were compared with baseline
images to determine the reduction in bacterial luminescence resulting
from treatment. RESULTS: The irrigation in both groups reduced the
bacterial counts by 71\% from the preirrigation levels. CONCLUSION:
Potable water reduced the bacterial load as effectively as normal
saline in our model.},
author = {Svoboda, Steven J and Owens, Brett D and Gooden, Heather A and Melvin, Michael L and Baer, David G and Wenke, Joseph C},
doi = {10.1097/TA.0b013e31816e3476},
institution = {US Army Institute of Surgical Research, Ft Sam Houston, Texas, USA.},
journal = {J Trauma},
keywords = { Animal; Goats; Irrigation, Microbial; Disease Models, methods; Sodium Chloride; Surgical Wound Infectio, microbiology/therapy; Water,Animals; Colony Count},
month = may,
number = {5},
pages = {1357--1359},
pmid = {18469661},
title = {{Irrigation with potable water versus normal saline in a contaminated musculoskeletal wound model.}},
url = {http://dx.doi.org/10.1097/TA.0b013e31816e3476},
volume = {64},
year = {2008}
}
@article{Ma1997,
abstract = {A novel boundary detection scheme based on \"{\i}¾“edge flow\"{\i}¾” is proposed
in this paper. This scheme utilizes a predictive coding model to
identify the direction of change in color and texture at each image
location at a given scale, and constructs an edge flow vector. By
iteratively propagating the edge flow, the boundaries can be detected
at image locations which encounter two opposite directions of flow
in the stable state. A user defined image scale is the only significant
control parameter that is needed by the algorithm. The scheme facilitates
integration of color and texture into a single framework for boundary
detection.},
annote = {The paper introduces a method of edge construction based on "Edge
flow", which is essentially a piece-by-piece reconstruction of the
edge based on its likely direction and strength. Directionality and
strength of edge at given point is determined via "predictive" power
or constancy of intensity in the direction of the edge, smoothed
with a gaussian kernel.},
author = {Ma, W Y and Manjynath, S},
journal = {IEEE},
keywords = { edge-detection, segmentation,computational,image processing},
title = {{Edge Flow: A Framework of Boundary Detection and Image Segmentation}},
year = {1997}
}
