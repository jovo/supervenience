journal = {Journal of neurophysiology},
keywords = {4-Aminopyridine,4-Aminopyridine: pharmacology,Acetylcholine,Acetylcholine: metabolism,Action Potentials,Action Potentials: drug effects,Action Potentials: physiology,Action Potentials: radiation effects,Animals,Anticoagulants,Anticoagulants: pharmacology,Calcium,Calcium: metabolism,Carbachol,Carbachol: pharmacology,Cholinergic Agonists,Cholinergic Agonists: pharmacology,Coloring Agents,Coloring Agents: pharmacology,Dendrites,Dendrites: drug effects,Dendrites: physiology,Dose-Response Relationship, Radiation,Feedback,Feedback: drug effects,Feedback: physiology,Heparin,Heparin: pharmacology,Membrane Potentials,Membrane Potentials: drug effects,Membrane Potentials: physiology,Membrane Potentials: radiation effects,Patch-Clamp Techniques,Potassium Channel Blockers,Potassium Channel Blockers: pharmacology,Pyramidal Cells,Pyramidal Cells: cytology,Rats,Rats, Sprague-Dawley,Ruthenium Red,Ruthenium Red: pharmacology,Visual Cortex,Visual Cortex: cytology},
