journal = {IEEE transactions on neural systems and rehabilitation engineering : a publication of the IEEE Engineering in Medicine and Biology Society},
keywords = {Action Potentials,Action Potentials: physiology,Animals,Computer Simulation,Humans,Models, Neurological,Nerve Net,Nerve Net: physiology,Neurons,Neurons: physiology,Nonlinear Dynamics,Synaptic Transmission,Synaptic Transmission: physiology},
month = aug,
number = {4},
pages = {336--52},
pmid = {18701382},
title = {{Nonlinear modeling of causal interrelationships in neuronal ensembles.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2729787\&tool=pmcentrez\&rendertype=abstract},
volume = {16},
