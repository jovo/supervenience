pages = {18014--9},
pmid = {17101989},
title = {{Bayesian estimation of the shape skeleton.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1635976\&tool=pmcentrez\&rendertype=abstract},
volume = {103},
year = {2006}
}
