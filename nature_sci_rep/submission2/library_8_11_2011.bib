journal = {Klin Onkol},
keywords = { Adjuvant; Deoxycytidine, Adjuvant; Rectal Neoplasms, Antineoplastic, Biological, analogs /\&/ derivatives/thera/peutic use; Fluorou, analogs /\&/ derivatives/therapeutic use; Humans; , analysis, drug therapy/radiotherapy/surgery; Antimetabolite, drug therapy/radiotherapy/surgery; Treatment Outc, therapeutic use; Chemotherapy,Adenocarcinoma},
number = {6},
