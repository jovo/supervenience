month = sep,
number = {9},
pages = {1035--40},
pmid = {18724362},
title = {{Photoswitchable fluorescent proteins enable monochromatic multilabel imaging and dual color fluorescence nanoscopy.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18724362},
volume = {26},
year = {2008}
}
