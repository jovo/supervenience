pmid = {11095747},
title = {{What is a moment? "Cortical" sensory integration over a brief interval.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=17676\&tool=pmcentrez\&rendertype=abstract},
volume = {97},
