keywords = {Animals,Databases, Factual,Drosophila,Drosophila: embryology,Imaging, Three-Dimensional,Imaging, Three-Dimensional: methods,Microscopy, Electron, Transmission,Microscopy, Electron, Transmission: methods},
month = jun,
number = {12},
pages = {i57--63},
pmid = {20529937},
title = {{As-rigid-as-possible mosaicking and serial section registration of large ssTEM datasets.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2881403\&tool=pmcentrez\&rendertype=abstract},
