journal = {Methods (San Diego, Calif.)},
keywords = {Action Potentials,Action Potentials: physiology,Animals,Calcium,Calcium: metabolism,Electrophysiology,Electrophysiology: methods,Fluorescent Dyes,Fura-2,Fura-2: analogs \& derivatives,Microscopy, Fluorescence,Microscopy, Fluorescence: methods,Neurons,Neurons: physiology,Pyramidal Cells,Pyramidal Cells: physiology,Rats,Rats, Sprague-Dawley,Software,Time Factors},
month = jun,
number = {2},
pages = {215--21},
pmid = {10356353},
title = {{Detecting action potentials in neuronal populations with calcium imaging.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10356353},
volume = {18},
year = {1999}
}
