journal = {Nature},
keywords = { Glutamate, analysis; Receptors, chemistry/metabolism, cytology/genetics/physiology; Calcium, genetics/physiology; Synapses, metabolism; Exploratory Behavior, metabolism; Interneurons, metabolism; Movement, metabolism; Odors, metabolism; Smell, physiology; Glutamic Acid, physiology; Neurons,Animals; Caenorhabditis elegans},
month = nov,
number = {7166},
pages = {63--70},
pmid = {17972877},
title = {{Dissecting a circuit for olfactory behaviour in Caenorhabditis elegans.}},
url = {http://dx.doi.org/8/nature06292},
volume = {450},
year = {2007}
}
