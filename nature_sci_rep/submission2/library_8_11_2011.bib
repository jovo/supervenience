keywords = {Acoustic Stimulation,Animals,Cats,Cochlear Nucleus,Cochlear Nucleus: physiology,Computational Biology,Hair Cells, Auditory, Inner,Hair Cells, Auditory, Inner: physiology,Models, Neurological,Nerve Fibers,Nerve Fibers: physiology,Neurons,Neurons: physiology,Noise,Vestibulocochlear Nerve,Vestibulocochlear Nerve: physiology},
number = {1},
