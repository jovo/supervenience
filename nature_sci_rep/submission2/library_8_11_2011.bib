month = mar,
number = {1},
pages = {5--17},
pmid = {20077162},
title = {{The Java Image Science Toolkit (JIST) for rapid prototyping and publishing of neuroimaging software.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2860951\&tool=pmcentrez\&rendertype=abstract},
