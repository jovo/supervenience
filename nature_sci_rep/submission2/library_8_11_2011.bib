pages = {880--6},
pmid = {15235606},
title = {{Underlying principles of visual shape selectivity in posterior inferotemporal cortex.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15235606},
volume = {7},
year = {2004}
}
