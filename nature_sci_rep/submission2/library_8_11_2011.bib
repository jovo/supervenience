doi = {10.3389/neuro.04.002.2009},
institution = {Howard Hughes Medical Institute, Department of Biological Sciences, Columbia University New York, NY, USA.},
journal = {Front Neural Circuits},
pages = {2},
pmid = {19506708},
title = {{RuBi-Glutamate: Two-Photon and Visible-Light Photoactivation of Neurons and Dendritic spines.}},
url = {http://dx.doi.org/10.3389/neuro.04.002.2009},
volume = {3},
