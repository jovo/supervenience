month = mar,
number = {December 2009},
pmid = {20205252},
title = {{Diffusion tensor imaging reliably differentiates patients with schizophrenia from healthy volunteers.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20205252},
volume = {00},
year = {2010}
}
