institution = {Cambridge Research Institute, Li Ka Shing Centre, Cambridge, UK. Claudia.Kutter@cancer.org.uk},
journal = {RNA Biol},
keywords = { Small Interfering, genetics/metabolism; Molecular Sequence Data; Plants, metabolism, metabolism; Cell Nucleus, metabolism; Humans; Mice; MicroRNAs, metabolism; RNA Interference; RNA,Animals; Base Sequence; Caenorhabditis elegans},
number = {4},
pages = {181--188},
pmid = {19182524},
title = {{miRNA, siRNA, piRNA: Knowns of the unknown.}},
volume = {5},
year = {2008}
