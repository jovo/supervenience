month = sep,
number = {3},
pages = {238--52},
pmid = {15492883},
title = {{Cross correlation by neurons of the medial superior olive: a reexamination.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15492883},
volume = {5},
year = {2004}
