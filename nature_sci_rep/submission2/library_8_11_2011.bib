doi = {10.1111/j.1600-6143.2008.02183.x},
institution = {Department of Medicine, University of Pennsylvania Medical Center, Philadelphia, PA, USA. detsai@mail.med.upenn.edu},
journal = {Am J Transplant},
keywords = { Human, Viral, adverse effects; Lymphoproliferative Disorders, blood/genetics, blood/genetics; Herpesvirus 4, blood/genetics; Viral Matrix Proteins, diagnosis/drug therapy; Epstein-Barr Virus Nuclea, genetics; Humans; Lung Transplantation, methods; Postoperative Complications, therapeutic use; DNA Primers; Epstein-Barr Virus , virology; Polymerase Chain Reaction, virology; Prospective Studies; RNA,Antiviral Agents},
