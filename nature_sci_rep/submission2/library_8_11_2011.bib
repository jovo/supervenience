issn = {1095-9572},
journal = {NeuroImage},
keywords = {brain,diffusion tensor imaging,human,post mortem,tractography},
month = apr,
pmid = {21473920},
title = {{Diffusion imaging of whole, post-mortem human brains on a clinical MRI scanner.}},
url = {http://dx.doi.org/10.1016/j.neuroimage.2011.03.070},
year = {2011}
}
