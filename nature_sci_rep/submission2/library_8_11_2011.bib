pages = {561--74},
pmid = {14711883},
title = {{Does the representation of time depend on the cerebellum? Effect of cerebellar stroke.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/14711883},
volume = {127},
year = {2004}
}
