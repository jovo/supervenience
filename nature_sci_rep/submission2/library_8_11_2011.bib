issn = {0896-6273},
journal = {Neuron},
keywords = {Action Potentials,Action Potentials: physiology,Animals,Female,Grasshoppers,Grasshoppers: physiology,Male,Mushroom Bodies,Mushroom Bodies: physiology,Neurons,Neurons: physiology,Odors,Reaction Time,Reaction Time: physiology,Smell,Smell: physiology},
