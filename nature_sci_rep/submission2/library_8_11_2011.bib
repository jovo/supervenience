keywords = {Animals,Biomechanics,Hand,Hand: physiology,Joints,Joints: physiology,Macaca mulatta,Mice,Models, Neurological,Motor Cortex,Motor Cortex: physiology,Movement,Movement: physiology,Muscle, Skeletal,Muscle, Skeletal: physiology,Neurons,Neurons: physiology,Posture,Posture: physiology,Torque},
month = may,
