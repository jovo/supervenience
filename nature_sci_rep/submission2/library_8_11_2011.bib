pages = {2900--16},
pmid = {17251368},
title = {{Properties of shape tuning of macaque inferior temporal neurons examined using rapid serial visual presentation.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17251368},
volume = {97},
year = {2007}
}
