month = apr,
number = {2},
pages = {140--7},
pmid = {17369035},
title = {{Transformation of shape information in the ventral pathway.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17369035},
volume = {17},
year = {2007}
}
